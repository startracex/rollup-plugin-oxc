{
  "name": "rollup-plugin-oxc",
  "version": "0.1.3",
  "description": "A Rollup plugin to transform, minify TypeScript and generate type declarations with oxc.",
  "type": "module",
  "scripts": {
    "build": "node build.ts"
  },
  "dependencies": {
    "@rollup/pluginutils": "^5.2.0",
    "oxc-minify": "^0.75.1",
    "oxc-resolver": "^11.4.0",
    "oxc-transform": "^0.75.1"
  },
  "devDependencies": {
    "@types/node": "^24.0.10",
    "rollup": "^4.44.2",
    "typescript": "^5.8.3"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./module/index.d.ts",
        "default": "./module/index.js"
      },
      "require": {
        "types": "./node/index.d.ts",
        "default": "./node/index.js"
      }
    },
    "./migrate.js": {
      "import": {
        "types": "./module/migrate.d.ts",
        "default": "./module/migrate.js"
      },
      "require": {
        "types": "./node/migrate.d.ts",
        "default": "./node/migrate.js"
      }
    },
    "./package.json": "./package.json",
    "./src/*": "./src/*"
  },
  "files": [
    "module",
    "node",
    "package.json"
  ],
  "keywords": [
    "rollup",
    "plugin",
    "oxc"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/startracex/rollup-plugin-oxc.git"
  },
  "author": "startracex",
  "license": "MIT",
  "packageManager": "pnpm@10.12.4"
}
