{
  "name": "rollup-plugin-oxc",
  "version": "0.3.2",
  "description": "A Rollup plugin to resolve, transform, minify TypeScript and generate type declarations with oxc.",
  "type": "module",
  "scripts": {
    "build": "node build.ts"
  },
  "dependencies": {
    "@rollup/pluginutils": "^5.2.0",
    "oxc-minify": "^0.77.2",
    "oxc-resolver": "^11.6.0",
    "oxc-transform": "^0.77.2",
    "tsconfck": "^3.1.6",
    "tsconfig-migrate": "^0.0.0"
  },
  "devDependencies": {
    "@types/node": "^24.0.15",
    "rollup": "^4.45.1",
    "typescript": "^5.8.3"
  },
  "exports": {
    ".": {
      "import": "./build/index.js",
      "require": "./build/index.cjs",
      "types": "./build/index.d.ts"
    },
    "./*.js": {
      "import": "./build/*.js",
      "require": "./build/*.cjs",
      "types": "./build/*.d.ts"
    },
    "./package.json": "./package.json",
    "./src/*": "./src/*"
  },
  "files": [
    "build",
    "src",
    "package.json"
  ],
  "keywords": [
    "rollup",
    "plugin",
    "oxc",
    "transform",
    "resolve",
    "minify",
    "typescript",
    "dts"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/startracex/rollup-plugin-oxc.git"
  },
  "author": "startracex",
  "license": "MIT",
  "packageManager": "pnpm@10.13.1"
}
